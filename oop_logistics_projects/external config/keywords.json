    ]
  }
}

{
  "app": {
    "name": "Disaster Logistics Management System",
    "version": "1.0.0",
    "language": "vi"
  },
  "analysis": {
    "sentiment_api_url": "http://localhost:5000",
    "sentiment_api_timeout_seconds": 30,
    "enable_sentiment_analysis": true
  },
  "collection": {
    "parallel_threads": 5,
    "collection_timeout_seconds": 60,
    "retry_attempts": 3
  },
  "preprocessing": {
    "enable_deduplication": true,
    "enable_location_extraction": true,
    "enable_severity_detection": true
  },
  "ui": {
    "window_width": 1200,
    "window_height": 800,
    "refresh_interval_seconds": 300
  }
}